{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n fmt,lint,client -c magenta,yellow,cyan \"npm run format:watch\" \"npm run lint:watch\" \"vite --host\"",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:watch": "chokidar 'src/**/*.{js,mjs,cjs,json}' -i 'node_modules' -i 'dist' -c \"eslint --cache --cache-location .eslintcache --fix {path} || true\"",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:watch": "chokidar '{index.html,src/**/*.{js,jsx,ts,tsx,css,html,md,scss}}' -i 'node_modules' -i 'dist' -c 'prettier --write {path}'"
  },
  "dependencies": {
    "@vis.gl/react-google-maps": "1.5.5",
    "lucide-react": "0.544.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@vitejs/plugin-react": "5.0.4",
    "autoprefixer": "^10.4.20",
    "chokidar-cli": "3.0.0",
    "concurrently": "9.2.1",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.23",
    "globals": "16.4.0",
    "postcss": "^8.4.49",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.17",
    "vite": "7.1.9"
  }
}