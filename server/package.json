{
  "name": "carbon-compass",
  "version": "1.0.0",
  "description": "A carbon compass to raise awareness about carbon emissions when travelling.",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently -n fmt,lint,server -c magenta,yellow,green \"npm run format:watch\" \"npm run lint:watch\" \"nodemon src/index.js\"",
    "start": "node src/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:watch": "chokidar 'src/**/*.{js,mjs,cjs,json}' -i 'node_modules' -i 'dist' -c \"eslint --cache --cache-location .eslintcache --fix {path} || true\"",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "format:watch": "chokidar 'src/**/*.{js,jsx,json,css,html}' -c 'prettier --write {path}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "1.12.2",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "chokidar-cli": "3.0.0",
    "concurrently": "9.2.1",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-security": "3.0.1",
    "globals": "16.4.0",
    "nodemon": "3.1.10",
    "prettier": "3.6.2"
  },
  "engines": {
    "node": ">=18"
  }
}
